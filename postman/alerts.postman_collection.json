{
  "info": {
    "name": "Regulus AML - Alerts",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://127.0.0.1:8081"
    },
    {
      "key": "request_id",
      "value": "postman-request-id"
    },
    {
      "key": "forwarded_for",
      "value": "127.0.0.1"
    },
    {
      "key": "device_id",
      "value": "postman-device"
    },
    {
      "key": "customer_id",
      "value": ""
    },
    {
      "key": "account_number",
      "value": ""
    },
    {
      "key": "transaction_id",
      "value": ""
    },
    {
      "key": "alert_id",
      "value": ""
    },
    {
      "key": "case_id",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "List Alerts",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-Request-ID",
            "value": "{{request_id}}"
          },
          {
            "key": "X-Forwarded-For",
            "value": "{{forwarded_for}}"
          },
          {
            "key": "X-Device-Id",
            "value": "{{device_id}}"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": "{{base_url}}/api/v1/alerts/?page=1&page_size=20"
      }
    },
    {
      "name": "Get Alert",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-Request-ID",
            "value": "{{request_id}}"
          },
          {
            "key": "X-Forwarded-For",
            "value": "{{forwarded_for}}"
          },
          {
            "key": "X-Device-Id",
            "value": "{{device_id}}"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": "{{base_url}}/api/v1/alerts/{{alert_id}}"
      }
    },
    {
      "name": "List Alerts By Account",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-Request-ID",
            "value": "{{request_id}}"
          },
          {
            "key": "X-Forwarded-For",
            "value": "{{forwarded_for}}"
          },
          {
            "key": "X-Device-Id",
            "value": "{{device_id}}"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": "{{base_url}}/api/v1/alerts/account/{{account_number}}?page=1&page_size=20"
      }
    }
  ]
}